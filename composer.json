{
    "name": "noardcode/laravel-uptime-monitor",
    "description": "Laravel uptime monitor.",
    "keywords": [
        "NoardCode",
        "Uptime",
        "Monitor",
        "Laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Hagg",
            "email": "chris@noardcode.nl",
            "homepage": "https://www.noardcode.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^7.24|^8.22"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Noardcode\\LaravelUptimeMonitor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Noardcode\\LaravelUptimeMonitor\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Noardcode\\LaravelUptimeMonitor\\LaravelUptimeMonitorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
